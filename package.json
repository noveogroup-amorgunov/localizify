    "lint": "eslint \"src/**/*.ts\" \"test/**/*.ts\"",
    "build": "webpack"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noveogroup-amorgunov/localizify.git"
  },
  "keywords": [
