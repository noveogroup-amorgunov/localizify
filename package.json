{
  "name": "localizify",
  "version": "3.0.0-beta",
  "description": "Localize your messages easily",
  "main": "dist/localizify.min.js",
  "types": "index.d.ts",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
